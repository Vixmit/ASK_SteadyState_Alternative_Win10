<Window x:Class="ASK_SteadyState_Alternative_Win10.AddNewUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASK_SteadyState_Alternative_Win10"
        mc:Ignorable="d"
        Title="AddNewUser" Height="450" Width="500">
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Height="29" Margin="10,10,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="110"/>
        <TextBlock x:Name="textBlock_Copy" HorizontalAlignment="Left" Height="29" Margin="10,40,0,0" TextWrapping="Wrap" Text="Description" VerticalAlignment="Top" Width="110"/>
        <TextBox x:Name="name" HorizontalAlignment="Left" Height="29" Margin="188,10,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="110"/>
        <TextBox x:Name="description" HorizontalAlignment="Left" Height="50" Margin="188,40,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="110"/>
        <Button x:Name="save" Content="Save" HorizontalAlignment="Left" Height="30" Margin="357,349,0,0" VerticalAlignment="Top" Width="90" Click="save_Click"/>
        <Button x:Name="cancel" Content="Cancel" HorizontalAlignment="Left" Height="30" Margin="211,349,0,0" VerticalAlignment="Top" Width="90" Click="cancel_Click"/>
        
    </Grid>
</Window>
